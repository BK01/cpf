<?xml version="1.0" encoding="UTF-8"?>
<beans
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:p="http://www.springframework.org/schema/p"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
  "
>

  <bean
    name="/CPF/CPF_BATCH_JOB_EXECUTION_GROUPS-htmlbuilder"
    class="ca.bc.gov.open.cpf.api.web.builder.BatchJobExecutionGroupUiBuilder"
    p:dataStore-ref="cpfDataStore"
  >
    <property name="pages">
      <list>
        <bean
           p:name="moduleAppJobList"
          class="com.revolsys.ui.web.config.Page"
         p:title="Execution Groups"
          p:path="/admin/modules/{moduleName}/apps/{businessApplicationName}/jobs/{batchJobId}/groups/" />
        <bean
          p:name="moduleAppJobView"
          class="com.revolsys.ui.web.config.Page"
          p:title="Batch Job {batchJobId} Execution Group {batchJobExecutionGroupId}"
          p:path="/admin/modules/{moduleName}/apps/{businessApplicationName}/jobs/{batchJobId}/groups/{batchJobExecutionGroupId}/" />
        <bean
         p:name="inputData"
           class="com.revolsys.ui.web.config.Page"
          p:path="/admin/modules/{moduleName}/apps/{businessApplicationName}/jobs/{batchJobId}/groups/{batchJobExecutionGroupId}/inputData" />
        <bean
          p:name="resultData"
          class="com.revolsys.ui.web.config.Page"
          p:path="/admin/modules/{moduleName}/apps/{businessApplicationName}/jobs/{batchJobId}/groups/{batchJobExecutionGroupId}/resultData" />
      </list>
    </property>

    <property name="serializers">
      <list>
        <bean
          p:name="INPUT_DATA_URL"
          class="com.revolsys.ui.html.serializer.key.UrlKeySerializer" />
        <bean
          p:name="RESULT_DATA_URL"
          class="com.revolsys.ui.html.serializer.key.UrlKeySerializer" />
        <bean
          class="com.revolsys.ui.html.serializer.key.ActionFormKeySerializer"
          p:name="inputData"
          p:label="Input Data Download"
          p:cssClass="ui-auto-button-disk"
          p:target="_top" />
        <bean
          class="com.revolsys.ui.html.serializer.key.ActionFormKeySerializer"
          p:name="resultData"
          p:label="Result Data Download"
          p:cssClass="ui-auto-button-disk"
          p:target="_top"
          p:enabledExpression="#COMPLETED_IND == 1" />
        <bean
          p:name="STARTED_IND"
          class="com.revolsys.ui.html.serializer.key.BooleanImageKeySerializer"
          p:label="Started" />
        <bean
          p:name="COMPLETED_IND"
          class="com.revolsys.ui.html.serializer.key.BooleanImageKeySerializer"
          p:label="Completed" />

        <bean
          p:name="NUM_SUBMITTED_REQUESTS"
          class="com.revolsys.ui.html.serializer.key.StringKeySerializer"
          p:label="Submitted Requests" />

        <bean
          p:name="NUM_COMPLETED_REQUESTS"
          class="com.revolsys.ui.html.serializer.key.StringKeySerializer"
          p:label="Completed Requests" />

        <bean
          p:name="NUM_FAILED_REQUESTS"
          class="com.revolsys.ui.html.serializer.key.StringKeySerializer"
          p:label="Failed Requests" />

        <bean
          p:name="WHO_CREATED"
          p:label="Created By"
          class="com.revolsys.ui.html.serializer.key.StringKeySerializer" />
        <bean
          p:name="WHEN_CREATED"
          p:label="Creation Time"
          class="com.revolsys.ui.html.serializer.key.DateFormatKeySerializer" />
        <bean
          p:name="WHO_UPDATED"
          p:label="Modified By"
          class="com.revolsys.ui.html.serializer.key.StringKeySerializer" />
        <bean
          p:name="WHEN_UPDATED"
          p:label="Modification Time"
          class="com.revolsys.ui.html.serializer.key.DateFormatKeySerializer" />
      </list>
    </property>

    <property name="views">
      <map>
        <entry key="moduleAppJobList">
          <list>
            <bean
              p:name="BATCH_JOB_EXECUTION_GROUP_ID"
              class="com.revolsys.ui.html.serializer.key.PageLinkKeySerializer"
              p:label="ID"
              p:pageName="moduleAppJobView" />
            <value>SEQUENCE_NUMBER</value>
            <value>STARTED_IND</value>
            <value>COMPLETED_IND</value>
            <value>NUM_SUBMITTED_REQUESTS</value>
            <value>NUM_COMPLETED_REQUESTS</value>
            <value>NUM_FAILED_REQUESTS</value>
            <value>WHEN_CREATED</value>
          </list>
        </entry>
        <entry key="moduleAppJobView">
          <list>
            <value>BATCH_JOB_EXECUTION_GROUP_ID</value>
            <bean
              class="com.revolsys.ui.html.serializer.key.PageLinkKeySerializer"
              p:name="BATCH_JOB_ID"
              p:label="Batch Job"
              p:pageName="moduleAppView"
              p:uiBuilder-ref="/CPF/CPF_BATCH_JOBS-htmlbuilder" />
            <value>SEQUENCE_NUMBER</value>
            <value>STARTED_IND</value>
            <value>COMPLETED_IND</value>
            <value>NUM_SUBMITTED_REQUESTS</value>
            <value>NUM_COMPLETED_REQUESTS</value>
            <value>NUM_FAILED_REQUESTS</value>
            <value>inputData</value>
            <value>INPUT_DATA_CONTENT_TYPE</value>
            <value>INPUT_DATA_URL</value>
            <bean
              p:name="BATCH_JOB_RESULT_ID"
              class="com.revolsys.ui.html.serializer.key.PageLinkKeySerializer"
              p:label="Result"
              p:pageName="moduleAppJobView"
              p:uiBuilder-ref="/CPF/CPF_BATCH_JOB_RESULTS-htmlbuilder" />
            <value>resultData</value>
            <value>WHO_CREATED</value>
            <value>WHEN_CREATED</value>
            <value>WHO_UPDATED</value>
            <value>WHEN_UPDATED</value>
          </list>
        </entry>
      </map>
    </property>
  </bean>

</beans>
