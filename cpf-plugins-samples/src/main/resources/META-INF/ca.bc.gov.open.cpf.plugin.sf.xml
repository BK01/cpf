<?xml version="1.0" encoding="UTF-8"?>
<beans
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:util="http://www.springframework.org/schema/util"
  xmlns:p="http://www.springframework.org/schema/p"
  xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/util
    http://www.springframework.org/schema/util/spring-util-3.0.xsd
  "
>
  <bean
    id="GeometryProjection"
    class="ca.bc.gov.open.cpf.plugin.samples.GeometryProjection"
    scope="prototype" />

  <bean
    id="Digest"
    class="ca.bc.gov.open.cpf.plugin.samples.Digest"
    scope="prototype" />

  <bean
    id="WebMapImage"
    class="ca.bc.gov.open.cpf.plugin.samples.WebMapImage"
    scope="prototype" />

  <bean
    id="Wait"
    class="ca.bc.gov.open.cpf.plugin.samples.Wait"
    scope="prototype" />

  <bean
    id="BcGeocoder"
    class="ca.bc.gov.open.cpf.plugin.samples.BcGeocoder"
    scope="prototype" />

  <bean
    id="UserInfo"
    class="ca.bc.gov.open.cpf.plugin.samples.UserInfo"
    scope="prototype" />
</beans>
