<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
  <properties
    file="build.properties" />

  <settings
    defaultResolver="default" />
  <caches
    useOrigin="true" />
  <resolvers
    m2compatible="true"
    changingPattern=".*-SNAPSHOT"
  >
    <chain
      name="default"
      returnFirst="true"
    >
      <filesystem
        name="webade"
      >
        <artifact
          pattern="/sw_ux/lib/gov.bc.ca/webade/4.2/[artifact]-[revision].[ext]" />
      </filesystem>

      <ibiblio
        name="local-m2"
        m2compatible="true"
        root="file://${user.home}/.m2/repository"
        checkmodified="true"
        changingPattern=".*-SNAPSHOT.*" />

      <ibiblio
        name="open.gov.bc.ca-m2"
        root="http://open.bcgov.revolsys.com/m2/repository"
        m2compatible="true"
        useMavenMetadata="false" />

      <ibiblio
        name="revolsys.com-m2"
        root="http://open.revolsys.com/m2/repository/"
        m2compatible="true"
        useMavenMetadata="false" />
    </chain>
  </resolvers>
</ivysettings>