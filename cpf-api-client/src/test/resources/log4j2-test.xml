<?xml version="1.0" encoding="UTF-8"?>
<Configuration
  strict="true"
  name="cpf-api-client-test"
>

  <Appenders>
    <Appender
      type="Console"
      name="Console"
    >
      <Layout
        type="PatternLayout"
        pattern="%p\t%c\t%m%n" />
    </Appender>
  </Appenders>

  <Loggers>
    <Root level="ERROR">
      <AppenderRef ref="Console" />
    </Root>
  </Loggers>

</Configuration>
