<?xml version="1.0" encoding="UTF-8" ?>
<orion-application
  xmlns="http://xmlns.oracle.com/oracleas/schema/oc4j-10_0.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
    http://xmlns.oracle.com/oracleas/schema/oc4j-10_0.xsd
    http://www.oracle.com/technology/oracleas/schema/orion-application-10_0.xsd
  "
  autocreate-tables="false"
  autodelete-tables="false"
  treat-zero-as-null="false"
>
  <web-module
    id="cpf-app"
    path="cpf-app.war" />
  <web-module
    id="cpf-worker"
    path="cpf-worker.war" />
  <library
    path="/sw_ux/lib/gov.bc.ca/webade/webadeconnections/" />

  <imported-shared-libraries>
    <remove-inherited
      name="apache.commons.logging" />
    <remove-inherited
      name="objectweb.asm" />
    <remove-inherited
      name="oracle.dms" />
    <remove-inherited
      name="oracle.gdk" />
    <remove-inherited
      name="oracle.http.client" />
    <remove-inherited
      name="oracle.jwsdl" />
    <remove-inherited
      name="oracle.persistence" />
    <remove-inherited
      name="oracle.sqlj" />
    <remove-inherited
      name="oracle.toplink" />
    <remove-inherited
      name="oracle.xml" />
    <remove-inherited
      name="oracle.ws.client" />
    <remove-inherited
      name="oracle.ws.core" />
    <remove-inherited
      name="oracle.ws.jaxrpc" />
    <remove-inherited
      name="oracle.ws.reliability" />
    <remove-inherited
      name="oracle.ws.security" />
    <remove-inherited
      name="oracle.ws.testpage" />
    <remove-inherited
      name="oracle.xml" />
    <remove-inherited
      name="oracle.xml.security" />
    <remove-inherited
      name="org.jgroups" />
    <remove-inherited
      name="soap" />
    <remove-inherited
      name="global.libraries" />
    <remove-inherited
      name="global.tag.libraries" />
  </imported-shared-libraries>
  
  <log>
    <file
      path="/apps_ux/logs/cpf/cpf-oc4j.log" />
  </log>
</orion-application>
