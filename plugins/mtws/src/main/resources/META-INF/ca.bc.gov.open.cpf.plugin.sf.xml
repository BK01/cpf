<?xml version="1.0" encoding="UTF-8"?>
<beans
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:util="http://www.springframework.org/schema/util"
  xmlns:p="http://www.springframework.org/schema/p"
  xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
    http://www.springframework.org/schema/util
    http://www.springframework.org/schema/util/spring-util-4.1.xsd
  "
>
  <bean
    id="mapTileByLocation"
    class="ca.bc.gov.mtws.MapTileByLocation"
    p:numBoundaryPoints="50"
    scope="prototype" />

  <bean
    id="mapTileByTileId"
    class="ca.bc.gov.mtws.MapTileByTileId"
    scope="prototype" />

  <util:map id="properties">
    <entry key="default">
      <map>
        <entry
          key="inputDataFileExtension"
          value="csv" />
        <entry
          key="resultDataFileExtension"
          value="kml" />
        <entry
          key="resultSrid"
          value="4326" />
        <entry
          key="resultNumAxis"
          value="2" />
        <entry
          key="resultScaleFactorZ"
          value="1" />
        <entry
          key="kmlPlaceMarkNameAttribute"
          value="mapTileId" />
      </map>
    </entry>
  </util:map>
</beans>
